{
  "name": "@xan105/is",
  "version": "2.5.1",
  "description": "Collection of various 'is ... ?' checks",
  "type": "module",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./lib/index.js"
    },
    "./assert": {
      "types": "./types/assert.d.ts",
      "default": "./lib/assert.js"
    },
    "./opt": {
      "types": "./types/opt.d.ts",
      "default": "./lib/opt.js"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "directories": {
    "lib": "lib",
    "test": "test --no-coverage"
  },
  "scripts": {
    "lint": "npx eslint --ignore-path .gitignore \"./**/*.{js,mjs}\"",
    "test": "tap"
  },
  "keywords": [
    "is",
    "should",
    "check",
    "assert"
  ],
  "author": {
    "name": "Anthony Beaumont",
    "email": "dev.xan105@outlook.be",
    "url": "https://xan105.com/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xan105/node-is.git"
  },
  "bugs": {
    "url": "https://github.com/xan105/node-is/issues"
  },
  "homepage": "https://github.com/xan105/node-is#readme",
  "devDependencies": {
    "eslint": "^8.17.0",
    "tap": "^16.2.0"
  },
  "dependencies": {
    "@xan105/error": "^1.3.1"
  }
}
